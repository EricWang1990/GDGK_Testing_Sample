<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sample.gdgk.testing_sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".demo1.LoginActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ex1.mvp.LoginActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".demo2.LoginActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".ex1.mvpvm.LoginActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".ex2.mvp.LoginActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".ex2.mvc.LoginActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".ex2.mvvm.LoginActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>